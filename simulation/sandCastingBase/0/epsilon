FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    wall
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    // inlet
    // {
    //     type            inletOutlet;
    //     inletValue      $internalField;
    //     value           $internalField;
    // }
    riser
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
}

sources
{
    injection
    {
        type            turbulentMixingLengthDissipationRate;
        mixingLength    0.0146;
    }
}



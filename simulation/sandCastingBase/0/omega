FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 11.1;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    wall
    {
        type            omegaWallFunction;
        value           $internalField;
    }

    inlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    riser
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
}


